{
  "name": "xfi.console.front",
  "version": "3.9.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https -H 0.0.0.0",
    "dev:ts": "yarn ts & yarn dev",
    "test": "jest --verbose --maxWorkers=3 --silent",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "pretty": "prettier --write ./**/*.ts",
    "lint": "next lint . --no-cache",
    "lint:fix": "next lint . --fix --no-cache",
    "prepare": "husky install",
    "postinstall": "patch-package && cross-os postinstall",
    "patch:packages": "patch-package"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "sh postinstall.unix.sh",
      "linux": "sh postinstall.unix.sh",
      "win32": "echo '// TODO: Run `sh postinstall.unix.sh` command in bash terminal'"
    }
  },
